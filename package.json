        "@babel/preset-react": "^7.17.12",
        "@emotion/react": "^11.9.0",
        "@emotion/styled": "^11.8.1",
        "@mui/icons-material": "^5.8.0",
        "@mui/material": "^5.8.0",
        "@mui/styles": "5.8.3",
        "@xstate/react": "^3.0.0",
        "formik": "^2.2.9",
        "moment": "^2.29.4",
        "react": "^18.2.0",
        "react-dom": "^18.0.0",
        "react-router-dom": "5",
